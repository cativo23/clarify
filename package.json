{
  "name": "clarify",
  "version": "1.0.0-alpha.7",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "db:migrate": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate",
    "db:rollback": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:rollback",
    "db:status": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:status",
    "db:fresh": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:fresh",
    "db:seed": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:seed",
    "db:wipe": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:wipe",
    "db:init": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:init",
    "db:refresh": "npm run db:wipe -- --force && npm run db:fresh -- --force && npm run db:migrate && npm run db:seed",
    "migrate:make": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:make"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/supabase": "^1.4.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@stripe/stripe-js": "^4.8.0",
    "bullmq": "^5.67.2",
    "chart.js": "^4.5.1",
    "ioredis": "^5.9.2",
    "js-tiktoken": "^1.0.21",
    "lucide-vue-next": "^0.563.0",
    "nuxt": "^3.15.1",
    "nuxt-security": "^2.5.1",
    "openai": "^4.77.3",
    "pdf-parse": "^1.1.1",
    "stripe": "^17.5.0",
    "vue": "latest",
    "zod": "^3"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.15.1",
    "@playwright/test": "^1.49.1",
    "@types/node": "^25.1.0",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/ui": "^2.1.9",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.8.0",
    "happy-dom": "^15.11.7",
    "supabase": "^2.76.9",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^10.4.0"
  }
}
