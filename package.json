{
  "name": "clarify",
  "version": "1.0.0-alpha.10.2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "db:migrate": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate",
    "db:rollback": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:rollback",
    "db:status": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:status",
    "db:fresh": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:fresh",
    "db:seed": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:seed",
    "db:wipe": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:wipe",
    "db:init": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts db:init",
    "db:refresh": "npm run db:wipe -- --force && npm run db:fresh -- --force && npm run db:migrate && npm run db:seed",
    "migrate:make": "NODE_OPTIONS='--import tsx' node scripts/migrate.ts migrate:make"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/supabase": "^2.0.4",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@stripe/stripe-js": "^8.8.0",
    "bullmq": "^5.70.0",
    "chart.js": "^4.5.1",
    "ioredis": "^5.9.3",
    "js-tiktoken": "^1.0.21",
    "lucide-vue-next": "^0.575.0",
    "nuxt": "^4.3.1",
    "nuxt-security": "^2.5.1",
    "openai": "^6.22.0",
    "pdf-parse": "^2.4.5",
    "stripe": "^20.3.1",
    "vue": "latest",
    "zod": "^4"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^4.0.0",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.3.0",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.8.0",
    "happy-dom": "^20.7.0",
    "supabase": "^2.76.12",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.4.0"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "glob": "^11.0.1",
    "archiver": "^7.0.1"
  },
  "engines": {
    "node": ">=24"
  }
}
